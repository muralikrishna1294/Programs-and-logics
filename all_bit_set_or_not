#include <stdio.h>

void all_bit_set(int num){

if(num!=0 && ((num+1)& num)==0){
    
    
    printf("\n all bits are set\n");
}else{
    
    printf("\n all bits are not set");
}
    
    
   
}

int main()
{
   
   all_bit_set(15);
   all_bit_set(10);
   
  
   
    return 0;
}
